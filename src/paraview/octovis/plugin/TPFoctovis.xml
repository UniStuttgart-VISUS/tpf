<ServerManagerConfiguration>
    <ProxyGroup name="filters">
        <!--

        Binary Star (Octo-Tiger).

        Identify and classify the stars.

        -->
        <SourceProxy name="Binary Star (Octo-Tiger)" class="tpf_binary_star" label="Binary Star (Octo-Tiger)">
            <Documentation>
                Identify and classify the stars.
            </Documentation>

            <InputProperty name="Input" label="Octree" command="SetInputConnection" port_index="0">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkPointSet"/>
                </DataTypeDomain>
                <InputArrayDomain attribute_type="point" name="density_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="point" name="density_accretor_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="point" name="density_donor_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="point" name="density_other_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="point" name="velocity_array" number_of_components="3"/>
                <InputArrayDomain attribute_type="point" name="gravitation_array" number_of_components="3"/>
                <InputArrayDomain attribute_type="point" name="cell_size_array" number_of_components="3"/>
                <InputArrayDomain attribute_type="field" name="xmin_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="field" name="xmax_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="field" name="ymin_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="field" name="ymax_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="field" name="zmin_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="field" name="zmax_array" number_of_components="1"/>
                <Documentation>
                    Points with ID, densities, velocities, gravitational potential, and meta information about the grid
                </Documentation>
            </InputProperty>

            <StringVectorProperty name="Density" label="Density" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="1">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="density_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Density within the cell
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Density Accretor" label="Density (Accretor)" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="2">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="density_accretor_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Density of the accretor within the cell
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Density Donor" label="Density (Donor)" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="3">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="density_donor_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Density of the donor within the cell
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Density Other" label="Density (Other)" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="4">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="density_other_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Density of other material within the cell
                </Documentation>
            </StringVectorProperty>

            <StringVectorProperty name="Velocity" label="Velocity" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="5">
                <ArrayListDomain name="array_list" attribute_type="Vectors" input_domain_name="velocity_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Velocity
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Gravitation" label="Gravitation" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="6">
                <ArrayListDomain name="array_list" attribute_type="Vectors" input_domain_name="gravitation_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Gravitation
                </Documentation>
            </StringVectorProperty>

            <StringVectorProperty name="CellSize" label="Cell size" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="0">
                <ArrayListDomain name="array_list" attribute_type="Vectors" input_domain_name="cell_size_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Cell sizes
                </Documentation>
            </StringVectorProperty>

            <StringVectorProperty name="Minimum X" label="Minimum X" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="7">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="xmin_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Left domain boundary; minimum x value
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Maximum X" label="Maximum X" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="8">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="xmax_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Right domain boundary; maximum x value
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Minimum Y" label="Minimum Y" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="9">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="ymin_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Lower domain boundary; minimum y value
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Maximum Y" label="Maximum Y" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="10">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="ymax_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Upper domain boundary; maximum y value
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Minimum Z" label="Minimum Z" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="11">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="zmin_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Back domain boundary; minimum z value
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Maximum Z" label="Maximum Z" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="12">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="zmax_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Front domain boundary; maximum z value
                </Documentation>
            </StringVectorProperty>

            <IntVectorProperty name="NumIterations" command="SetNumIterations" label="Number of iterations" number_of_elements="1" default_values="5">
                <Documentation>
                    Number of iteration steps
                </Documentation>
            </IntVectorProperty>
            <DoubleVectorProperty name="DensityCutoff" command="SetDensityCutoff" label="Density cutoff" number_of_elements="1" default_values="1.0">
                <Documentation>
                    Cells with a density below this cutoff are discarded as stars
                </Documentation>
            </DoubleVectorProperty>

            <PropertyGroup label="Point data">
                <Property name="Density" />
                <Property name="Density Accretor" />
                <Property name="Density Donor" />
                <Property name="Density Other" />
                <Property name="Velocity" />
                <Property name="Gravitation" />
                <Property name="CellSize" />
            </PropertyGroup>

            <PropertyGroup label="Domain data">
                <Property name="Minimum X" />
                <Property name="Maximum X" />
                <Property name="Minimum Y" />
                <Property name="Maximum Y" />
                <Property name="Minimum Z" />
                <Property name="Maximum Z" />
            </PropertyGroup>

            <PropertyGroup label="Parameters">
                <Property name="NumIterations" />
                <Property name="DensityCutoff" />
            </PropertyGroup>

            <OutputPort name="Octree" index="0" id="octree">
                <Documentation>
                    Copy of the input octree, appended with classification and information for each cell.
                </Documentation>
            </OutputPort>
            <OutputPort name="Stars" index="1" id="stars">
                <Documentation>
                    Positions of the stars with their properties.
                </Documentation>
            </OutputPort>

            <Hints>
                <ShowInMenu category="Two-Phase Flow"/>
            </Hints>
        </SourceProxy>

        <!--

        Flow Field (Octo-Tiger).

        Compute stream-, streak- or pathlines on the octree from Octo-Tiger simulation results.

        -->
        <SourceProxy name="Flow Field (Octo-Tiger)" class="tpf_flow_field_octree" label="Flow Field (Octo-Tiger)">
            <Documentation>
                Compute stream-, streak- or pathlines on the octree from Octo-Tiger simulation results.
            </Documentation>

            <InputProperty name="Input" label="Octree" command="SetInputConnection" port_index="0">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkPointSet"/>
                </DataTypeDomain>
                <InputArrayDomain attribute_type="point" name="id_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="point" name="velocity_array" number_of_components="3"/>
                <InputArrayDomain attribute_type="field" name="xmin_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="field" name="xmax_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="field" name="ymin_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="field" name="ymax_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="field" name="zmin_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="field" name="zmax_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="field" name="time_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="field" name="omega_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="point" name="classification_array" number_of_components="1" optional="1"/>
                <InputArrayDomain attribute_type="point" name="property_arrays" optional="1"/>
                <Documentation>
                    Points with ID, velocities, and meta information about the grid and its rotation
                </Documentation>
            </InputProperty>
            <InputProperty name="Seed" command="SetInputConnection" port_index="1">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkPolyData"/>
                </DataTypeDomain>
                <Documentation>
                    Input seed for stream-, streak and path lines
                </Documentation>
            </InputProperty>
            <InputProperty name="Stars" command="SetInputConnection" port_index="2">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkPointSet"/>
                </DataTypeDomain>
                <InputArrayDomain attribute_type="point" name="velocity_array" number_of_components="3"/>
                <InputArrayDomain attribute_type="point" name="rotation_array" number_of_components="3"/>
                <Documentation>
                    Identified star positions and respective properties
                </Documentation>
            </InputProperty>

            <StringVectorProperty name="ID" label="Point ID" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="0">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="id_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    IDs indicating the point position within the octree
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Velocity" label="Velocity" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="1">
                <ArrayListDomain name="array_list" attribute_type="Vectors" input_domain_name="velocity_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Velocities
                </Documentation>
            </StringVectorProperty>

            <StringVectorProperty name="Minimum X" label="Minimum X" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="4">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="xmin_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Left domain boundary; minimum x value
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Maximum X" label="Maximum X" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="5">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="xmax_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Right domain boundary; maximum x value
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Minimum Y" label="Minimum Y" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="6">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="ymin_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Lower domain boundary; minimum y value
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Maximum Y" label="Maximum Y" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="7">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="ymax_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Upper domain boundary; maximum y value
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Minimum Z" label="Minimum Z" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="8">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="zmin_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Back domain boundary; minimum z value
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Maximum Z" label="Maximum Z" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="9">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="zmax_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Front domain boundary; maximum z value
                </Documentation>
            </StringVectorProperty>

            <StringVectorProperty name="Time" label="Time" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="10">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="time_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Timestep of the data
                </Documentation>
            </StringVectorProperty>

            <StringVectorProperty name="Grid Rotation" label="Angular frequency" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="11">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="omega_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Grid rotation given by the angular frequency omega
                </Documentation>
            </StringVectorProperty>

            <IntVectorProperty name="Method" command="SetMethod" label="Method" number_of_elements="1" default_values="0">
                <EnumerationDomain name="enum">
                    <Entry value="0" text="Streamlines"/>
                    <Entry value="1" text="Streaklines"/>
                    <Entry value="2" text="Pathlines"/>
                </EnumerationDomain>
                <Documentation>
                    Method for the computation of the flow lines
                </Documentation>
            </IntVectorProperty>
            <IntVectorProperty name="NumAdvections" command="SetNumAdvections" label="Number of lines" number_of_elements="1" default_values="20">
                <Documentation>
                    Number of advection steps
                </Documentation>
            </IntVectorProperty>
            <IntVectorProperty name="ForceFixedTimeStep" command="SetForceFixedTimeStep" label="Force fixed time step" number_of_elements="1" default_values="0">
                <BooleanDomain name="bool"/>
                <Documentation>
                    Force a fixed time step for streak- and pathline computation
                </Documentation>
            </IntVectorProperty>
            <DoubleVectorProperty name="StreamTimeStep" command="SetStreamTimeStep" label="Fixed time step" number_of_elements="1" default_values="1.0">
                <Hints>
                    <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="ForceFixedTimeStep" value="1" />
                </Hints>
                <Documentation>
                    Time step size for streamline computation; and for streak- and pathline computation in case of a fixed time step
                </Documentation>
            </DoubleVectorProperty>

            <IntVectorProperty name="LocalityMethod" command="SetLocalityMethod" label="Locality method" number_of_elements="1" default_values="1">
                <EnumerationDomain name="enum">
                    <Entry value="0" text="None"/>
                    <Entry value="1" text="Rotation around origin"/>
                    <Entry value="2" text="Star velocity"/>
                    <Entry value="3" text="Star rotation"/>
                    <Entry value="4" text="Rigid body"/>
                </EnumerationDomain>
                <Documentation>
                    Method for the definition of locality
                </Documentation>
            </IntVectorProperty>
            <IntVectorProperty name="ForceFixedFrequency" command="SetForceFixedFrequency" label="Force fixed frequency" number_of_elements="1" default_values="0">
                <BooleanDomain name="bool"/>
                <Hints>
                    <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="LocalityMethod" value="1" />
                </Hints>
                <Documentation>
                    Override the omega rotation given by the dataset
                </Documentation>
            </IntVectorProperty>
            <DoubleVectorProperty name="FrequencyOmega" command="SetFrequencyOmega" label="Rotational frequency (omega)" number_of_elements="1" default_values="1.0">
                <Hints>
                    <PropertyWidgetDecorator type="CompositeDecorator">
                        <Expression type="and">
                            <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="LocalityMethod" value="1" />
                            <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="ForceFixedFrequency" value="1" />
                        </Expression>
                    </PropertyWidgetDecorator>
                </Hints>
                <Documentation>
                    Omega rotation when overriding the value given by the dataset
                </Documentation>
            </DoubleVectorProperty>
            <StringVectorProperty name="StarClassification" label="Star Classification" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="12">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="classification_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Hints>
                    <PropertyWidgetDecorator type="CompositeDecorator">
                        <Expression type="or">
                            <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="LocalityMethod" value="2" />
                            <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="LocalityMethod" value="3" />
                            <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="LocalityMethod" value="4" />
                        </Expression>
                    </PropertyWidgetDecorator>
                </Hints>
                <Documentation>
                    Classification of each cell, indicating its corresponding star
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="StarVelocity" label="Star Velocity" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="13">
                <ArrayListDomain name="array_list" attribute_type="Vectors" input_domain_name="velocity_array">
                    <RequiredProperties>
                        <Property name="Stars" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Hints>
                    <PropertyWidgetDecorator type="CompositeDecorator">
                        <Expression type="or">
                            <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="LocalityMethod" value="2" />
                            <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="LocalityMethod" value="4" />
                        </Expression>
                    </PropertyWidgetDecorator>
                </Hints>
                <Documentation>
                    Velocity of the stars at their center of mass
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="StarRotation" label="Star Rotation" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="14">
                <ArrayListDomain name="array_list" attribute_type="Vectors" input_domain_name="rotation_array">
                    <RequiredProperties>
                        <Property name="Stars" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Hints>
                    <PropertyWidgetDecorator type="CompositeDecorator">
                        <Expression type="or">
                            <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="LocalityMethod" value="3" />
                            <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="LocalityMethod" value="4" />
                        </Expression>
                    </PropertyWidgetDecorator>
                </Hints>
                <Documentation>
                    Rotation of the stars around their center of mass, given as angular velocity
                </Documentation>
            </StringVectorProperty>

            <StringVectorProperty name="Arrays" command="GetArraySelection" number_of_elements_per_command="1" repeat_command="1" si_class="vtkSIDataArraySelectionProperty">
                <ArrayListDomain name="array_list" input_domain_name="property_arrays">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Interpolate at particle positions and store values along stream- or pathlines
                </Documentation>
                <Hints>
                    <ArraySelectionWidget icon_type="point"/>
                </Hints>
            </StringVectorProperty>

            <PropertyGroup label="Point data">
                <Property name="ID" />
                <Property name="Velocity" />
            </PropertyGroup>

            <PropertyGroup label="Domain data">
                <Property name="Minimum X" />
                <Property name="Maximum X" />
                <Property name="Minimum Y" />
                <Property name="Maximum Y" />
                <Property name="Minimum Z" />
                <Property name="Maximum Z" />
                <Property name="Time" />
                <Property name="Grid Rotation" />
            </PropertyGroup>

            <PropertyGroup label="Computation">
                <Property name="Method" />
                <Property name="NumAdvections" />
                <Property name="ForceFixedTimeStep" />
                <Property name="StreamTimeStep" />
            </PropertyGroup>

            <PropertyGroup label="Locality">
                <Property name="LocalityMethod" />
                <Property name="ForceFixedFrequency" />
                <Property name="FrequencyOmega" />
                <Property name="StarClassification" />
                <Property name="StarVelocity" />
                <Property name="StarRotation" />
            </PropertyGroup>

            <PropertyGroup label="Property arrays">
                <Property name="Arrays" />
            </PropertyGroup>

            <Hints>
                <ShowInMenu category="Two-Phase Flow"/>
            </Hints>
        </SourceProxy>

        <!--

        Seed (Octo-Tiger).

        Create seed from the octree, e.g., at an iso surface.

        -->
        <SourceProxy name="Seed (Octo-Tiger)" class="tpf_seed_octree" label="Seed (Octo-Tiger)">
            <Documentation>
                Create seed from the octree, e.g., at an iso surface.
            </Documentation>

            <InputProperty name="Input" label="Octree" command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkPointSet"/>
                </DataTypeDomain>
                <InputArrayDomain attribute_type="point" name="id_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="point" name="rho_array" number_of_components="1" optional="1"/>
                <InputArrayDomain attribute_type="field" name="xmin_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="field" name="xmax_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="field" name="ymin_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="field" name="ymax_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="field" name="zmin_array" number_of_components="1"/>
                <InputArrayDomain attribute_type="field" name="zmax_array" number_of_components="1"/>
                <Documentation>
                    Points with ID, and optionally the density for the iso surface
                </Documentation>
            </InputProperty>

            <StringVectorProperty name="ID" label="Point ID" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="0">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="id_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    IDs indicating the point position within the octree
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Density" label="Density" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="1">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="rho_array" none_string="None">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Hints>
                    <NoDefault />
                </Hints>
                <Documentation>
                    Density (rho)
                </Documentation>
            </StringVectorProperty>

            <StringVectorProperty name="Minimum X" label="Minimum X" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="2">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="xmin_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Left domain boundary; minimum x value
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Maximum X" label="Maximum X" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="3">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="xmax_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Right domain boundary; maximum x value
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Minimum Y" label="Minimum Y" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="4">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="ymin_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Lower domain boundary; minimum y value
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Maximum Y" label="Maximum Y" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="5">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="ymax_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Upper domain boundary; maximum y value
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Minimum Z" label="Minimum Z" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="6">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="zmin_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Back domain boundary; minimum z value
                </Documentation>
            </StringVectorProperty>
            <StringVectorProperty name="Maximum Z" label="Maximum Z" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0" default_values="7">
                <ArrayListDomain name="array_list" attribute_type="Scalars" input_domain_name="zmax_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>
                    Front domain boundary; maximum z value
                </Documentation>
            </StringVectorProperty>

            <IntVectorProperty name="SeedMethod" command="SetSeedMethod" label="Seed method" number_of_elements="1" default_values="0">
                <EnumerationDomain name="enum">
                    <Entry value="0" text="Octree leaves"/>
                    <Entry value="1" text="Isosurface"/>
                </EnumerationDomain>
                <Documentation>
                    Seeding strategy
                </Documentation>
            </IntVectorProperty>
            <IntVectorProperty name="SeedOffset" command="SetSeedOffset" label="Seed offset" number_of_elements="1" default_values="0" panel_visibility="advanced">
                <Documentation>
                    Start the seeding in the n-th cell
                </Documentation>
            </IntVectorProperty>
            <IntVectorProperty name="SeedSize" command="SetSeedSize" label="Seed size" number_of_elements="1" default_values="-1" panel_visibility="advanced">
                <Documentation>
                    Only seed in a limited number of cells
                </Documentation>
            </IntVectorProperty>
            <DoubleVectorProperty name="Isovalue" command="SetIsovalue" label="Isovalue" number_of_elements="1" default_values="0.5">
                <Hints>
                    <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="SeedMethod" value="1" />
                </Hints>
                <Documentation>
                    Isovalue for density-based seeding (isosurface)
                </Documentation>
            </DoubleVectorProperty>
            <IntVectorProperty name="Predicate" command="SetPredicate" label="Predicate" number_of_elements="1" default_values="0">
                <EnumerationDomain name="enum">
                    <Entry value="0" text="Both"/>
                    <Entry value="1" text="Larger"/>
                    <Entry value="2" text="Smaller"/>
                </EnumerationDomain>
                <Documentation>
                    Only seed if value is larger/smaller than the isovalue
                </Documentation>
            </IntVectorProperty>

            <PropertyGroup label="Point data">
                <Property name="ID" />
                <Property name="Density" />
            </PropertyGroup>

            <PropertyGroup label="Domain data">
                <Property name="Minimum X" />
                <Property name="Maximum X" />
                <Property name="Minimum Y" />
                <Property name="Maximum Y" />
                <Property name="Minimum Z" />
                <Property name="Maximum Z" />
            </PropertyGroup>

            <PropertyGroup label="Seeding">
                <Property name="SeedMethod" />
                <Property name="SeedOffset" />
                <Property name="SeedSize" />
                <Property name="Isovalue" />
                <Property name="Predicate" />
            </PropertyGroup>

            <Hints>
                <ShowInMenu category="Two-Phase Flow"/>
            </Hints>
        </SourceProxy>
    </ProxyGroup>
</ServerManagerConfiguration>
